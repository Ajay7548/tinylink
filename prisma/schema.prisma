generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Link {
  id            Int      @id @default(autoincrement())
  fullUrl       String
  code          String   @unique
  clicks        Int      @default(0)
  lastClickedAt DateTime?
  createdAt     DateTime @default(now())
}
